{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUsername": {
      "value": "yourAdminUsername"
    },
    "adminPassword": {
      "value": "yourAdminPassword"
    },
    "dnsLabelPrefix": {
      "value": "yourDNSLabelPrefix"
    },
    "publicIpName": {
      "value": "yourPublicIPName"
    },
    "publicIPAllocationMethod": {
      "value": "Dynamic"
    },
    "publicIpSku": {
      "value": "Basic"
    },
    "OSVersion": {
      "value": "2022-datacenter-azure-edition"
    },
    "vmSize": {
      "value": "Standard_D2s_v5"
    },
    "location": {
      "value": "yourLocation"
    },
    "vmName": {
      "value": "yourVMName"
    },
    "securityType": {
      "value": "TrustedLaunch"
    }
  }
}